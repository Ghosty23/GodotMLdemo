; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="HideAndSeek_PPO_MULTI_AGENT2"
run/main_scene="res://main_menu.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1400
window/size/height=700
window/size/resizable=false
window/driver/fallback_to_gles2=true

[editor_plugins]

enabled=PoolStringArray( "zylann.hterrain" )

[gdnative]

singletons=[ "res://addons/pythonscript.gdnlib" ]

[rendering]

vram_compression/import_etc=true
quality/filters/use_nearest_mipmap_filter=true
